        app:srcCompat="@drawable/ico_cliente" />

    <ImageButton
        android:id="@+id/btnProveedores"
        android:layout_width="130dp"
        android:layout_height="90dp"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:background="?attr/colorControlHighlight"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:srcCompat="@drawable/ico_proveedor" />

    <ImageButton
        android:id="@+id/btnAcumulados"
        android:layout_width="130dp"
        android:layout_height="90dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="20dp"
        android:background="?attr/colorControlHighlight"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:srcCompat="@drawable/ico_acumula" />

    <ImageButton
        android:id="@+id/btnAjustes"
        android:layout_width="130dp"
        android:layout_height="90dp"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:background="?attr/colorControlHighlight"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:srcCompat="@drawable/ico_ajuste" />

    <ImageButton
        android:id="@+id/btnGuardarC"
        android:layout_width="130dp"
        android:layout_height="90dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="20dp"
        android:background="?attr/colorControlHighlight"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:srcCompat="@drawable/ico_salir" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:text="Clientes"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnClientes" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="131dp"
        android:layout_height="20dp"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:text="Proveedores"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnProveedores" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="129dp"
        android:layout_height="20dp"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:text="Inventario"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnInventario" />

</android.support.constraint.ConstraintLayout>